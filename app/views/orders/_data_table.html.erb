            <!-- <div class="table-row-group">
              <%#= render partial: "/orders/table/row", collection: orders, as: :order %>
            </div> -->
            <% orders.each do |order| %>
              <div class="table-row-group odd:bg-gray-50 odd:hover:shadow even:bg-white even:hover:shadow">
                <%= tag.div id: dom_id(order), class: "table-row" do %>
                  <% primary_col_css_classes = "table-cell py-2 align-middle text-center text-sm font-medium text-gray-900 h-full group" %>
                  <% target_top_link_to_css_classes = "text-indigo-600 hover:text-indigo-800 hover:cursor-pointer" %>
                  <%= tag.div class: "pl-3 #{primary_col_css_classes}" do %>
                    <%= link_to "View", order, class:target_top_link_to_css_classes, target: "_top" %>
                    <%= link_to "Edit", edit_order_path(order), data: {turbo_frame: "modal" }, class: target_top_link_to_css_classes if authorized_internal_user? %>
                  <% end %>
                  <%#= tag.div order.id, class: primary_col_css_classes %>
                  <%= tag.div class: primary_col_css_classes do %>
                  <%= tag.div order.id, class: '' %>
                    <%= tag.div class: 'inline-block max-w-full' do %>
                      <%= tag.div class: 'mx-3 text-gray-600' do %>
                        <%= render "/statuses/status", statusable: order %>
                      <% end %>
                    <% end %>
                  <% end %>
                  <% unless @vendor %>
                    <%= tag.div class: primary_col_css_classes do %>
                      <%= render "inline_edit", model: order, attribute: :order_sequence do %>
                        <%= order.order_sequence %>
                      <% end %>
                    <% end %>
                  <% end %>
                  <%= tag.div class: primary_col_css_classes do %>
                    <%= render "orders/inline_edit_dept", order: order, attribute: :dept do %>
                      <%= tag.div (order.dept? ? order.dept : 'n/a'), class: "text-gray-900" %>
                    <% end %>
                  <% end %>
                  <%= tag.div class: primary_col_css_classes do %>
                    <%= link_to order.purchaser.name, purchaser_orders_path(order.purchaser), class: target_top_link_to_css_classes, target: "_top" %>
                  <% end %>
                  <%= tag.div class: primary_col_css_classes do %>
                    <%= tag.div class: "inline-block" do %>
                        <%= authorized_internal_user? ? (link_to order.vendor.name, vendor_orders_path(order.vendor), class: target_top_link_to_css_classes, target: "_top") : (tag.p order.vendor.name, class: primary_col_css_classes) %>
                    <% end %>
                  <% end %>
                  <%= tag.div class: primary_col_css_classes do %>
                    <%= render "inline_edit", model: order, attribute: :po_number do %>
                      <%= order.po_number %>
                    <% end %>
                  <% end %>
                  <%= tag.div class: primary_col_css_classes do %>
                    <%= render "inline_edit", model: order, attribute: :date_recieved do %>
                      <%= order.try(:date_recieved) ? order.date_recieved.try(:strftime,"%m/%d/%Y") : 'n/a' %>
                    <% end %>
                  <% end %>
                  <%= tag.div class: primary_col_css_classes do %>
                    <% unless order.try(:order_content).nil? %>
                        <%= render partial: "/orders/table/order_content_packaging_materials_td", locals: {
                        order: order,
                        target_top_link_to_css_classes: "text-indigo-600 hover:font-semi-bold hover:text-indigo-800 hover:cursor-pointer hover:shadow-sm",
                        } %>
                    <% else %>
                    <%= (tag.div "No order content", class: "w-full rounded-md px-2 py-1 text-red-600 font-bold") %>
                    <% end %>
                  <% end %>
                  <%= tag.div class: primary_col_css_classes do %>
                    <%= render "inline_edit", model: order, attribute: :courrier do %>
                      <%= order.courrier.present? ? order.courrier : 'n/a' %>
                    <% end %>
                  <% end %>
                  <%= tag.div class: primary_col_css_classes do %>
                    <%= render "inline_edit", model: order, attribute: :date_delivered do %>
                      <%= order.try(:date_delivered) ? order.date_delivered.try(:strftime,"%m/%d/%Y") : 'n/a' %>
                    <% end %>
                  <% end %>
                <% end %>
              </div>
            <% end %>

