
<%# if Current.request_variant == :turbo_frame  %>

  <% active_result_css_classes = "bg-gray-100 bg-slate-100" %>
  <% active_result_css_classes_on_hover = "hover:bg-gray-100 hover:bg-slate-100" %>

  <li class="group flex cursor-default select-none items-center rounded-md p-2 hover:bg-gray-100 hover:bg-slate-100" id="option-1" role="option" tabindex="-1">

    <span class="ml-3 flex-auto truncate">
      <%= result.class %>
      <%= result.id %>
    </span>

    <%# Not Active: "hidden" %>
    <svg class="ml-3 h-5 w-5 flex-none text-gray-400 hidden group-hover:block" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
      <path fill-rule="evenodd" d="M7.21 14.77a.75.75 0 01.02-1.06L11.168 10 7.23 6.29a.75.75 0 111.04-1.08l4.5 4.25a.75.75 0 010 1.08l-4.5 4.25a.75.75 0 01-1.06-.02z" clip-rule="evenodd" />
    </svg>
  </li>

<%# end %>



